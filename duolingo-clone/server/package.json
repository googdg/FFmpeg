{
  "name": "duolingo-clone-server",
  "version": "1.0.0",
  "description": "Backend API server for Duolingo Clone",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "migrate": "ts-node src/database/migrate.ts",
    "migrate:up": "npm run migrate up",
    "migrate:status": "npm run migrate status",
    "migrate:rollback": "npm run migrate rollback",
    "seed": "ts-node src/database/seed.ts",
    "seed:run": "npm run seed run",
    "seed:clear": "npm run seed clear",
    "seed:reset": "npm run seed reset",
    "seed:stats": "npm run seed stats",
    "db:setup": "npm run migrate:up && npm run seed:run",
    "db:reset": "npm run migrate:up && npm run seed:reset"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^6.1.5",
    "morgan": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "redis": "^4.6.5",
    "joi": "^17.9.1",
    "express-rate-limit": "^6.7.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/pg": "^8.6.6",
    "@types/compression": "^1.7.2",
    "@types/uuid": "^9.0.1",
    "@types/node": "^18.15.11",
    "@types/jest": "^29.5.1",
    "typescript": "^5.0.4",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.38.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "prettier": "^2.8.7"
  },
  "keywords": ["duolingo", "language-learning", "api", "express", "typescript"],
  "author": "DuoClone Team",
  "license": "MIT"
}